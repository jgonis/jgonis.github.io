{
  "compilerOptions": {
    "target": "ES6",
    "inlineSourceMap": true,
    "removeComments": true,
    "watch": true,
    "outFile": "../build/start.js",
    "strictNullChecks": true,
    "noImplicitAny": true,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "lib": ["dom", "es6"]
  }
}